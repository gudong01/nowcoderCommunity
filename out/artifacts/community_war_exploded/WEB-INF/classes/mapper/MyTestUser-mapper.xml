<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nowcoder.community.dao.MyTestUserMapper">
    <select id="selectByName" resultType="MyTestUser">
        select * from testuser where myname = #{myname}
    </select>

    <insert id="insertData">
        insert into testuser (myname,myage)
        values(#{myname},#{myage})
    </insert>
</mapper>